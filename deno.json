{
  "name": "@dreamer/config",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/mod.ts",
    "./client": "./src/client/mod.ts"
  },
  "publish": {
    "include": [
      "src/mod.ts",
      "src/client/mod.ts",
      "LICENSE",
      "NOTICE",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/service": "jsr:@dreamer/service@^1.0.0",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.10",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.15"
  }
}
